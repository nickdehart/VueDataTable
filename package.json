{
  "name": "VueBoilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-client": "cross-env NODE_ENV=production webpack -p --env.production",
    "build-client-watch": "webpack -w --env.development",
    "start": "node app",
    "start-dev": "cross-env NODE_ENV=development npm-run-all --parallel start-server build-client-watch",
    "start-server": "nodemon app",
    "webpack": "cross-env NODE_ENV=production webpack --env.production"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "~1.14.1",
    "bootstrap": "^3.3.7",
    "express": "~4.13.3",
    "express-session": "~1.11.3",
    "jquery": "^3.3.1",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "nodemon": "^1.13.3",
    "npm-run-all": "^4.1.2",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.6.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  }
}
